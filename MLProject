name: My MLFlow Test Project

docker_env:
   image: pytorch/pytorch:latest

entry_points:
  main:
    parameters:
      regularization: {type: float, default: 0.1}
    command: "python main.py -r {regularization} {data_file}"
  validate:
    parameters:
      data_file: path
    command: "python validate.py {data_file}"